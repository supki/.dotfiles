*.perl-lib: $template.urxvt.perllib$
*.tabbed.tabbar-fg: 3
*.tabbed.tabbar-bg: 8
*.tabbed.tab-fg: 8
*.tabbed.tab-bg: 3
*.tabbed.new-button: no
*.tabbed.autohide: yes
*.tabbed.title: no
*.iso14755: False

*.background: $template.urxvt.background_$
*.foreground: #ffffff

*.font: -*-terminus-medium-r-*-*-12-*-*-*-*-*-iso10646-1
*.boldFont: -*-terminus-medium-r-*-*-12-*-*-*-*-*-iso10646-1

*.scrollBar: False
*.cursorBlink: True
*.cursorUnderline: True

*.saveLines: 12000

*.color0:  #000000
*.color1:  #ff6a6a
*.color2:  #bbbb66
*.color3:  #dd9977
*.color4:  #cccccc
*.color5:  #eeccaa
*.color6:  #00aaaa
*.color7:  #ffffff
*.color8:  $template.urxvt.background_$
*.color9:  #ff5555
*.color10: #55ff55
*.color11: #ffff55
*.color12: #5555ff
*.color13: #ff55ff
*.color14: #55ffff
*.color15: #ffffff
*.underlineColor: #eeccaa

*.urlLauncher: $template.urxvt.browser$
*.perl-ext-common: default,matcher,tabbedex,keyboard-select,url-select

*.matcher.pattern.0: http[s]*:\\/\\/[^ "]+"?
*.matcher.button: 1

*.keysym.C-s: perl:keyboard-select:activate
*.keysym.C-u: perl:url-select:select_next

vimus.background: #212121

vimus.saveLines: 12000
